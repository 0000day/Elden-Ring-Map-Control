{
  "manifest_version": 3,
  "name": "Elden Ring Interactive Map",
  "version": "1.0",
  "description": "Ã–ffnet die Elden Ring interaktive Karte und passt deren Stile an.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://eldenring.wiki.fextralife.com/Interactive+Map"],
      "js": ["content_script.js"]
    }
  ]
}